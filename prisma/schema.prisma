generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model server {
  id    String  @id @unique @default(cuid())
  label String  @default("Unnamed")
  root  String?
  jar   String  @default("server.jar")
  args  String  @default("-Xms256M")
}
